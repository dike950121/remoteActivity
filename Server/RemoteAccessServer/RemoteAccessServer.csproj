    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyTitle>Remote Access Server</AssemblyTitle>
    <AssemblyDescription>Command and Control Server for Remote Access Payload</AssemblyDescription>
